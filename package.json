{
  "name": "wordle",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "server": "ts-node server.ts",
    "build": "ng build",
    "build:prod": "ng build --configuration prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate:models": "openapi-generator-cli generate -i openapi.yaml -g typescript-angular -o src/app/domain --additional-properties fileNaming=kebab-case --generate-alias-as-model --global-property models"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
